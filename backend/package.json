{
  "name": "veterinary-consultation-backend",
  "version": "1.0.0",
  "description": "Enterprise-grade Veterinary Doctor Consultation Platform Backend",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node --project tsconfig.json src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "migrate": "ts-node --project tsconfig.json src/utils/migrationRunner.ts",
    "seed": "ts-node --project tsconfig.json src/utils/seedDatabase.ts"
  },
  "keywords": [
    "veterinary",
    "consultation",
    "saas",
    "telemedicine"
  ],
  "author": "Your Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "pg": "^8.11.3",
    "redis": "^4.6.12",
    "dotenv": "^16.3.1",
    "winston": "^3.11.0",
    "joi": "^17.11.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.1.2",
    "uuid": "^9.0.1",
    "axios": "^1.6.7",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "class-validator": "^0.14.0",
    "class-transformer": "^0.5.1",
    "reflect-metadata": "^0.1.13",
    "ioredis": "^5.3.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.6",
    "@types/jest": "^29.5.11",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.7",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.2",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "supertest": "^6.3.3",
    "@types/supertest": "^6.0.2",
    "prettier": "^3.1.1"
  }
